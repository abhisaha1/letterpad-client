{
  "name": "letterpad-client",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k 'next -p 3001' 'graphql-codegen -w'",
    "build": "yarn codegen && next build",
    "codegen": "graphql-codegen",
    "type-check": "tsc --noEmit",
    "start": "next start -p 3001"
  },
  "dependencies": {
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "next": "12.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-jsx-plugin-sass": "^1.0.0",
    "tiny-js-modal": "^1.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typescript": "^1.17.10",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^3.2.2",
    "@tailwindcss/typography": "^0.5.0",
    "@types/graphql": "^14.5.0",
    "@types/react": "^16.9.52",
    "autoprefixer": "^10.4.0",
    "concurrently": "^5.3.0",
    "node-sass": "^6.0.0",
    "postcss": "^8.4.5",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.1.2",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.4.4"
  }
}
